{
  "compilerOptions": {
    "target": "ESNext",
    "module": "nodenext",
    "lib": [
      "ESNext"
    ],
    "moduleResolution": "nodenext",
    "strict": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "declaration": true,
    "types": [
      "node"
    ]
  },
}